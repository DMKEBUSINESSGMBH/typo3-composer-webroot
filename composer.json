{
    "name" : "dmk/typo3-composer-webroot",
    "description" : "The TYPO3 Project webroot",
    "type" : "project",
    "license" : "proprietary",
    "version" : "0.9.5",
    "authors" : [
        {
            "name" : "Michael Wagner",
            "email" : "michael.wagner@dmk-ebusiness.de",
            "role" : "Developer"
        }
    ],
    "support": {
        "email" : "dev@dmk-ebusiness.de",
        "source" : "https://github.com/DMKEBUSINESSGMBH/typo3-composer-webroot",
        "issues" : "https://github.com/DMKEBUSINESSGMBH/typo3-composer-webroot/issues"
    },
    "repositories" : {
        "dmknet" : {
            "type" : "composer",
            "url" : "https://packages.dmknet.de"
        },
        "typo3" : {
            "type" : "composer",
            "url" : "https://composer.typo3.org/"
        }
    },
    "require" : {
        "typo3/minimal" : "~9.5@stable",
        "typo3/cms-core" : "~9.5",
        "typo3/minimal": "~9.5",
        "typo3/cms-tstemplate": "~9.5",
        "typo3/cms-seo": "~9.5",
        "typo3/cms-form": "~9.5",
        "typo3/cms-beuser": "~9.5",
        "typo3/cms-redirects": "~9.5",
        "typo3/cms-viewpage": "~9.5",

        "gridelementsteam/gridelements" : "@dev",
        "sjbr/static-info-tables" : "~6.7@stable"
        
    },
    "minimum-stability" : "stable",
    "extra" : {
        "typo3/cms" : {
            "cms-package-dir" : "{$vendor-dir}/typo3/cms",
            "web-dir" : "./htdocs"
        }
    },
    "require-dev" : {
        "mikey179/vfsStream" : "^1"
    }
}
